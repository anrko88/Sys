    Protected Sub btnGenerar_Click(ByVal sender As Object, ByVal e As System.EventArgs) Handles btnGenerar.Click
        'AAE - Agrego variables 
        Dim strFechaIni As String = txtFechaInicial.Value
        Dim strFechaFin As String = txtFechaFinal.Value
        Dim strFlag As String = "1"

        Dim objUtilNTx As New LUtilNTX

        Dim sbResultado As New StringBuilder("")
        Dim strResultado As String
        'Tablas
        Dim table As DataTable 'variable para recorrer las tablas

        Try
           
            table = GCCUtilitario.DeserializeObject(Of DataTable)(objUtilNTx.ListaRegistroVentas(strFlag, strFechaIni, strFechaFin))
            sbResultado.Append(generarExcelHeaders(strFechaIni, strFechaFin))
            sbResultado.Append(generarExcelLineas(table))
            strResultado = sbResultado.ToString

            If Not String.IsNullOrEmpty(strResultado) Then
                Session("stream") = strResultado
                ScriptManager.RegisterStartupScript(Me.Page, GetType(String), "FrameScript", "window.open('Streamdownload.aspx?archivo=Registro_Ventas.xls&aplicacion=application/excel');", True)
            End If
        Catch ex As Exception
            Throw ex
        End Try

    End Sub

    Protected Sub btnGenerar2_Click(ByVal sender As Object, ByVal e As System.EventArgs) Handles btnGenerar2.Click
        'AAE - Agrego variables 
        Dim strFechaIni As String = txtFechaInicial.Value
